        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gestcom.Views"
        mc:Ignorable="d"
        Title="edit_pesee" Height="300" Width="500">
    <Grid>
        <Label x:Name="lblMois" Content="Mois :" HorizontalAlignment="Left" Margin="42,38,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cbxMois" HorizontalAlignment="Left" Margin="95,42,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblAnnee" Content="Annee :" HorizontalAlignment="Left" Margin="240,39,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbxAnnee" MaxLength="2" HorizontalAlignment="Left" Margin="300,46,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="55"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="215,99,0,0" VerticalAlignment="Top" Click="Button_Click"/>

    </Grid>
</Window>
